{
    "manifest_version": 2,
    
    "name": "Tab Saver",
    "description": "This extension saves all tabs in the currently opened window to a .txt file",
    "version": "1.1.1",
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "menu.html"
    },
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "downloads"
    ]
}